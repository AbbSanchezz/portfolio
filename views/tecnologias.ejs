  <link rel="shortcut icon" href="resources/css/imagenes/favicon.ico">
  <link href="resources/css/style-login.css" rel="stylesheet">
  <title>Tecnologias</title>
</head>

<body>
  <div class="row formulario">
    <div class="col-md-4">
      <h4 class="formulario">Agregar tecnologias</h4>
      <form action="tecnologias" method="post" class="formulario">
        <label for="tituloTecnologia">Titulo</label>
        <input class="form-control log" name="tituloTecnologia" type="text">

        <label for="descripcionTecnologia">Descripcion</label>
        <input class="form-control log" name="descripcionTecnologia" type="text">

        <input class="btn-login btn btn-primary" type="submit" value="Guardar">
      </form>
    </div>
  </div>
</body>

</html>